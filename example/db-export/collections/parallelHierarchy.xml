            <sc:state id="Held">
              <sc:transition target="Graded" event="issueCertificates"/>
            </sc:state>
            <sc:state id="Graded"/>
          </sc:state>
        </sc:scxml>
      </elements>
      <parentLevels>
        <level ref="course"/>
      </parentLevels>
    </level>
  </levels>
